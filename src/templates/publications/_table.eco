<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Authors</th>
      <th>Year</th>
      <th>Journal</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    <% for row in @data: %>
      <tr>
        <td></td>
        <td><%= row.authors %></td>
        <td><%= row.year %></td>
        <td><%= row.journal %></td>
        <td><%= row.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>